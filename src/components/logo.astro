---
import { Picture } from "astro:assets";
import logo from "./logo.png";

interface Props {
  width: number;
  class?: string;
}

const { width, class: cls = "" } = Astro.props;
---

<Picture
  src={logo}
  formats={["webp", "png"]}
  alt=""
  width={width}
  quality="max"
  densities={[1.5, 2, 3]}
  class={`logo ${cls}`}
/>
